{
  "name": "frontend",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "webpack serve --config webpack.config.js --mode development --open",
    "dev": "webpack serve --config webpack.config.js --mode development",
    "build": "webpack --mode production",
    "preview": "serve -s dist -l 4000",
    "clean": "rimraf dist .webpack_cache",
    "lint": "eslint src --ext .js,.jsx",
    "format": "prettier --write \"src/**/*.{js,jsx,json,css,scss}\"",
    "test": "jest",
    "postinstall": "npm run prepare",
    "fix-deps": "npx npm-force-resolutions && npm install --legacy-peer-deps",
    "env:validate": "node -e \"const config = require('./src/config/env.js'); if (!config.API.URL) throw new Error('REACT_APP_API_URL manquant'); console.log('âœ… Configuration valide')\"",
    "prestart": "npm run env:validate",
    "predev": "npm run env:validate",
    "prebuild": "npm run env:validate",
    "analyze": "webpack-bundle-analyzer dist/stats.json",
    "prepare": "husky install",
    "preinstall": "npx npm-force-resolutions"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm test"
    }
  },
  "dependencies": {
    "@babel/runtime-corejs3": "^7.24.1",
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.14.19",
    "@mui/material": "^5.14.19",
    "axios": "^1.6.2",
    "dotenv": "^16.3.1",
    "ethers": "^6.9.0",
    "framer-motion": "^10.16.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-intersection-observer": "^9.5.3",
    "react-router-dom": "^6.20.1",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.28.0",
    "@babel/plugin-transform-class-properties": "^7.27.1",
    "@babel/plugin-transform-private-methods": "^7.27.1",
    "@babel/plugin-transform-private-property-in-object": "^7.27.1",
    "@babel/plugin-transform-runtime": "^7.28.0",
    "@babel/preset-env": "^7.28.0",
    "@babel/preset-react": "^7.27.1",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.11",
    "@testing-library/react": "^14.1.1",
    "@types/react": "^18.2.41",
    "@types/react-dom": "^18.2.18",
    "assert": "^2.1.0",
    "autoprefixer": "^10.4.16",
    "babel-loader": "^9.1.3",
    "babel-plugin-module-resolver": "^5.0.2",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "browserify-zlib": "^0.2.0",
    "buffer": "^6.0.3",
    "compression-webpack-plugin": "^10.0.0",
    "core-js": "^3.33.2",
    "crypto-browserify": "^3.12.0",
    "css-loader": "^6.8.1",
    "css-minimizer-webpack-plugin": "^7.0.2",
    "cssnano": "^6.0.1",
    "dotenv-webpack": "^8.0.1",
    "eslint": "^8.56.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "favicons-webpack-plugin": "^6.0.0",
    "html-webpack-plugin": "^5.5.3",
    "https-browserify": "^1.0.0",
    "husky": "^8.0.3",
    "image-minimizer-webpack-plugin": "^3.8.3",
    "imagemin": "^8.0.1",
    "imagemin-gifsicle": "^7.0.0",
    "imagemin-mozjpeg": "^9.0.0",
    "imagemin-pngquant": "^9.0.2",
    "imagemin-svgo": "^10.0.1",
    "imagemin-webp": "^7.0.0",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.1",
    "mini-css-extract-plugin": "^2.7.6",
    "os-browserify": "^0.3.0",
    "path-browserify": "^1.0.1",
    "postcss": "^8.4.31",
    "postcss-loader": "^7.3.3",
    "postcss-preset-env": "^9.1.4",
    "prettier": "^3.1.1",
    "process": "^0.11.10",
    "react-refresh": "^0.14.2",
    "rimraf": "^5.0.5",
    "sass": "^1.69.5",
    "sass-loader": "^13.3.2",
    "serve": "^14.2.1",
    "stream-browserify": "^3.0.0",
    "stream-http": "^3.2.0",
    "style-loader": "^3.3.3",
    "terser-webpack-plugin": "^5.3.9",
    "url": "^0.11.0",
    "util": "^0.12.5",
    "webpack": "^5.89.0",
    "webpack-bundle-analyzer": "^4.9.1",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1",
    "webpack-manifest-plugin": "^5.0.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "testEnvironment": "jsdom",
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1",
      "\\.(css|scss)$": "identity-obj-proxy"
    },
    "transformIgnorePatterns": [
      "/node_modules/(?!(@mui|@emotion|react-router-dom|framer-motion)/)"
    ]
  },
  "lint-staged": {
    "**/*.{js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "**/*.{css,scss}": [
      "prettier --write"
    ]
  },
  "resolutions": {
    "braces": "^3.0.2",
    "cross-spawn": "^6.0.6",
    "decompress": "^4.2.1",
    "got": "^11.8.5",
    "lodash.template": "^4.5.0",
    "semver": "^7.5.4",
    "url-regex": "^5.0.0",
    "trim-newlines": "^3.0.1",
    "ajv": "^8.11.0",
    "ajv-keywords": "^5.1.0",
    "imagemin-mozjpeg": "^10.0.0",
    "imagemin-pngquant": "^10.0.0",
    "imagemin-gifsicle": "^7.0.0"
  }
}
